	Journal = {OPERATIONS RESEARCH},
	Number = {6},
	Pages = {1407-1420},
	Title = {{Dynamic Pricing Without Knowing the Demand Function: Risk Bounds and Near-Optimal Algorithms}},
	Url = {http://or.journal.informs.org/cgi/content/abstract/57/6/1407},
	Volume = {57},
	Year = {2009},
	Bdsk-File-1 = {},
