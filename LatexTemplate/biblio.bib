	Eprint = {http://or.journal.informs.org/cgi/reprint/55/6/1039.pdf},
	Journal = {OPERATIONS RESEARCH},
	Keywords = {dynamic pricing, inventory control, customer choice model},
	Number = {6},
	Pages = {1039-1057},
	Title = {{Pricing and Manufacturing Decisions When Demand Is a Function of Prices in Multiple Periods}},
